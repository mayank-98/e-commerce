\nimport { Link } from 'react-router-dom'\nimport axios from 'axios'\n\nfunction Header() {\n    const state = useContext(GlobalState)\n    const [isLogged, setIsLogged] = state.userAPI.isLogged\n    const [isAdmin, setIsAdmin] = state.userAPI.isAdmin\n    const [cart] = state.userAPI.cart\n\n    const logoutUser = async () => {\n        await axios.get('/user/logout')\n        localStorage.clear()\n        window.location.href = \"/\";\n    }\n\n    const adminRouter = () => {\n        return (\n            <>\n                <li><Link to=\"/create_product\" >Create Product</Link></li>\n                <li><Link to=\"/category\" >Categories</Link></li>\n            </>\n        )\n    }\n\n    const loggedRouter = () => {\n        return (\n            <>\n                <li><Link to=\"/history\" >History</Link></li>\n                <li><Link to=\"/\" onClick={logoutUser} >Logout</Link></li>\n            </>\n        )\n    }\n\n    return (\n        <header>\n            <div className=\"menu\">\n                <img src={Menu} alt=\"\" width=\"30\" />\n            </div>\n            <div className=\"logo\">\n                <h1>\n                    <Link to=\"/\">{isAdmin ? 'Admin' : 'MyShop'}</Link>\n                </h1>\n            </div>\n            <ul>\n                <li><Link to=\"/\">{isAdmin ? 'Products' : 'Shop'}</Link></li>\n                {isAdmin && adminRouter()}\n                {\n                    isLogged ? loggedRouter() : <li><Link to=\"/login\">Login ✥ Register</Link></li>\n                }\n\n                <li>\n                    <img src={Close} alt=\"\" width=\"30\" className=\"menu\" />\n                </li>\n            </ul>\n\n            {\n                isAdmin ? ''\n                    : <div className=\"cart-icon\">\n                        <span>{cart.length}</span>\n                        <Link to=\"/cart\">\n                            <img src={Cart} alt=\"\" width=\"30\" />\n                        </Link>\n                    </div>\n            }\n\n\n        </header>\n    )\n}\n\nexport default Header\n","/Users/mayank/Desktop/app/new/e-commerce/client/src/components/mainpages/auth/Login.js",[],"/Users/mayank/Desktop/app/new/e-commerce/client/src/components/mainpages/auth/Register.js",[],"/Users/mayank/Desktop/app/new/e-commerce/client/src/components/mainpages/products/Products.js",["84"],"import React, { useContext } from 'react'\nimport { GlobalState } from '../../../GlobalState'\nimport ProductItem from '../utils/productItem/ProductItem'\nimport Loading from '../utils/loading/Loading'\n\nfunction Products() {\n    const state = useContext(GlobalState)\n    const [products] = state.productsAPI.products\n    const [isAdmin] = state.userAPI.isAdmin\n    const [cart] = state.userAPI.cart\n\n    return (\n        <>\n            <div className=\"products\">\n                {\n                    products.map(product => {\n                        return <ProductItem key={product._id} product={product}\n                            isAdmin={isAdmin} />\n                    })\n                }\n            </div>\n            {products.length === 0 && <Loading />}\n        </>\n    )\n}\n\nexport default Products\n","/Users/mayank/Desktop/app/new/e-commerce/client/src/components/mainpages/cart/Cart.js",[],"/Users/mayank/Desktop/app/new/e-commerce/client/src/components/mainpages/utils/not_found/NotFound.js",[],"/Users/mayank/Desktop/app/new/e-commerce/client/src/api/ProductsAPI.js",["85"],"import React, { useState, useEffect } from 'react'\nimport axios from 'axios'\n\nfunction ProductsAPI() {\n    const [products, setProducts] = useState([])\n\n    const getProducts = async () => {\n        const res = await axios.get('/api/products')\n        setProducts(res.data.products)\n    }\n\n    useEffect(() => {\n        getProducts()\n    }, [])\n    return {\n        products: [products, setProducts]\n    }\n}\n\nexport default ProductsAPI\n","/Users/mayank/Desktop/app/new/e-commerce/client/src/components/mainpages/utils/productItem/ProductItem.js",["86"],"import React from 'react'\nimport { Link } from 'react-router-dom'\nimport BtnRender from './BtnRender'\n\nfunction ProductItem({ product, isAdmin }) {\n    return (\n        <div className=\"product_card\">\n            {\n                isAdmin && <input type=\"checkbox\" checked={product.checked} />\n            }\n            <img src={product.images.url} alt=\"\" />\n\n            <div className=\"product_box\">\n                <h2 title={product.title}>{product.title}</h2>\n                <span>₹{product.price}</span>\n                <p>${product.description}</p>\n            </div>\n\n            <BtnRender product={product} />\n        </div>\n    )\n}\n\nexport default ProductItem\n","/Users/mayank/Desktop/app/new/e-commerce/client/src/components/mainpages/detailProduct/DetailProduct.js",[],"/Users/mayank/Desktop/app/new/e-commerce/client/src/components/mainpages/utils/productItem/BtnRender.js",["87"],"import React, { useContext } from 'react'\nimport { Link } from 'react-router-dom'\nimport { GlobalState } from '../../../../GlobalState'\n\nfunction BtnRender({ product }) {\n    const state = useContext(GlobalState)\n    const [products] = state.productsAPI.products\n    const [isAdmin] = state.userAPI.isAdmin\n    const addCart = state.userAPI.addCart\n\n    return (\n        <div className=\"row_btn\">\n            {\n                isAdmin ?\n                    <>\n                        <Link id=\"btn_buy\" to=\"#!\">Delete</Link>\n                        <Link onClick={() => {\n                            window.scrollTo({ top: 0, behavior: 'smooth' })\n                        }} id=\"btn_view\" to={`/edit_product/${product._id}`} >Edit\n                        </Link>\n                    </>\n                    : <>\n                        <Link id=\"btn_buy\" to=\"#!\" onClick={() => addCart(product)} >Buy</Link>\n                        <Link onClick={() => {\n                            window.scrollTo({ top: 0, behavior: 'smooth' })\n                        }} id=\"btn_view\" to={`/detail/${product._id}`} >View\n                        </Link>\n                    </>\n            }\n        </div>\n    )\n}\n\nexport default BtnRender\n","/Users/mayank/Desktop/app/new/e-commerce/client/src/components/mainpages/utils/loading/Loading.js",[],"/Users/mayank/Desktop/app/new/e-commerce/client/src/api/UserAPI.js",["88"],"/Users/mayank/Desktop/app/new/e-commerce/client/src/components/mainpages/history/OrderHistory.js",[],"/Users/mayank/Desktop/app/new/e-commerce/client/src/components/mainpages/history/OrderDetails.js",[],{"ruleId":"89","replacedBy":"90"},{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","severity":1,"message":"94","line":11,"column":22,"nodeType":"95","messageId":"96","endLine":11,"endColumn":33},{"ruleId":"93","severity":1,"message":"97","line":12,"column":21,"nodeType":"95","messageId":"96","endLine":12,"endColumn":31},{"ruleId":"93","severity":1,"message":"98","line":10,"column":12,"nodeType":"95","messageId":"96","endLine":10,"endColumn":16},{"ruleId":"93","severity":1,"message":"99","line":1,"column":8,"nodeType":"95","messageId":"96","endLine":1,"endColumn":13},{"ruleId":"93","severity":1,"message":"100","line":2,"column":10,"nodeType":"95","messageId":"96","endLine":2,"endColumn":14},{"ruleId":"93","severity":1,"message":"101","line":7,"column":12,"nodeType":"95","messageId":"96","endLine":7,"endColumn":20},{"ruleId":"93","severity":1,"message":"99","line":2,"column":8,"nodeType":"95","messageId":"96","endLine":2,"endColumn":13},"no-native-reassign",["102"],"no-negated-in-lhs",["103"],"no-unused-vars","'setIsLogged' is assigned a value but never used.","Identifier","unusedVar","'setIsAdmin' is assigned a value but never used.","'cart' is assigned a value but never used.","'React' is defined but never used.","'Link' is defined but never used.","'products' is assigned a value but never used.","no-g